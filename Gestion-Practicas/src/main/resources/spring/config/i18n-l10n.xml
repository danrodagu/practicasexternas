		<value>/views/misc/messages</value>
		<value>/views/security/messages</value>		
		<value>/views/welcome/messages</value>
		
		<value>/views/actor/messages</value>
		<value>/views/administrativo/messages</value>
		<value>/views/alumno/messages</value>
		<value>/views/carpeta/messages</value>
		<value>/views/coordinador/messages</value>
		<value>/views/documento/messages</value>
		<value>/views/mensaje/messages</value>
		<value>/views/noticia/messages</value>
		<value>/views/oferta/messages</value>
		<value>/views/tutor/messages</value>
		<value>/views/valoracion/messages</value>
		
		
	</util:list>

</beans>